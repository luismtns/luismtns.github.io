{
  "name": "luisbovo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "homepage": "http://luismtns.github.io/",
  "scripts": {
    "build": "rm -rf build && NODE_ENV=production APP_ENV=production ./node_modules/.bin/webpack -p",
    "deploy": "gh-pages -d build",
    "start": "set APP_ENV=development&&./node_modules/.bin/webpack-dev-server",
    "start-win": "./node_modules/.bin/webpack-dev-server"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.7.0",
    "history": "4.10.1",
    "js-sha1": "^0.6.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-grid": "^4.0.4",
    "react-lazy-load-image-component": "^1.5.1",
    "react-redux": "7.2.2",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^7.0.3",
    "react-typist": "^2.0.5",
    "redux": "^4.0.5",
    "redux-form": "8.3.7",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.13.7",
    "reset-css": "^5.0.1",
    "uuid": "^8.3.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-object-assign",
      [
        "module-resolver",
        {
          "alias": {
            "@app": "./src",
            "@components": "./src/components",
            "@config": "./src/config",
            "@containers": "./src/containers",
            "@data": "./src/data",
            "@utils": "./src/utils",
            "@services": "./src/services",
            "@views": "./src/views",
            "@globalConfig": "./config"
          }
        }
      ]
    ]
  }
}
